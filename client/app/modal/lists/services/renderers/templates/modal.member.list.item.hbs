<div class="member-item cursor_pointer {{css.memberItem}} {{css.admin}}" style="height: {{itemHeight}}px;">
  <img class="member-profile-image" src="{{profileImage}}"/>
  <span class="member-name inline-overflow-ellipsis {{css.memberName}}">{{{html.userName}}}</span>
  {{#if isAdmin}}
    <span class="member-auth-label">{{text.admin}}</span>
  {{else if isInactive}}
    <span class="resend-invitation _sendInvitation"><i class="icon-mail"></i><span>{{text.invitationResend}}</span></span>
  {{/if}}
  {{#if isShowStar}}
    <i class="icon-star-off right-star opac-zero {{starClass}}"></i>
  {{/if}}
</div>
